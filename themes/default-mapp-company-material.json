            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "[JAVASCRIPT] - Numbers",
            "scope": "source.js constant",
            "settings": {
                "foreground": "#d6498b"
            }
        },
        {
            "name": "Meta",
